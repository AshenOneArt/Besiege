function delay(time)
{
    var stop = 0
    setTimeout(time,function(){stop = 1})
    while(stop != 1){}
    return stop
}

function caculateFireTime(inputID,TargetAngle,OutputID_Fire)
{
    var vectorInfo = in(inputID)
    if (vectorInfo)
        var rotate = 180 - ((vectorInfo + 0.5)%1) * 360
    if(PrintOrNot)print(Name + 'rotate  ' + rotate)
}

while(delay(0.1))
{
    caculateFireTime(0,90,1)
}
